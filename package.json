{
  "name": "@web-lite/snackbar",
  "version": "0.0.1",
  "private": false,
  "description": "React easy snack-bar",
  "main": "build/index.js",
  "module": "build/index.js",
  "sideEffects": false,
  "files": [
    "build/",
    "esm/",
    "src/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Amir.H Ebrahimi",
    "email": "amirhosseinebrahimi77@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/weblite-wapps/snackbar"
  },
  "bugs": {
    "url": "https://github.com/weblite-wapps/snackbar/issues"
  },
  "homepage": ".",
  "scripts": {
    "lint": "eslint src/**/*.js",
    "lint:fix": "yarn lint --fix",
    "dev": "react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "full:build": "react-scripts build",
    "clean": "rimraf lib esm"
  },
  "husky": {
